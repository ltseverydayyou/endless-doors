-- Script
local femboy = game:GetDescendants()
for i = 1, #femboy do local v = femboy[i]
if v.Name == Part then
v:Destroy()
end
end


-- Name
local Part = "Fire"
 
-- Script
local femboy = game:GetDescendants()
for i = 1, #femboy do local v = femboy[i]
if v.Name == Part then
v:Destroy()
end
end
wait(5)
end
	end    
})

