Tab2:AddToggle({
	Name = "Bit Aura",
	Default = false,
	Save = true,
	Callback = function(Value)
		_G.bits = Value
while _G.bits == true do
for i, v in pairs(game.workspace:GetDescendants()) do
        if v.Name == "Bit" then
            fireproximityprompt(v.ProximityPrompt)
        end
    end
wait(0.1)
end
	end    
})

