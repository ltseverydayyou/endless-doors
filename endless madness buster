Tab1:AddToggle({
	Name = "Bypass Broken Lamps",
	Default = false,
	Callback = function(Value)
		_G.lemp = Value
while _G.lemp == true do
for i, v in pairs(game.workspace:GetDescendants()) do
        if v.Name == "BrokenLamp" then
            v:Destroy()
        end
    end
wait(0.1)
end
	end    
})

local Section = Tab2:AddSection({
	Name = "Warning: Annoying Sounds [still in beta]"
})

Tab2:AddToggle({
	Name = "Bit AutoFarm",
	Default = false,
	Callback = function(Value)
		_G.bits = Value
while _G.bits == true do
for i, v in pairs(game.workspace:GetDescendants()) do
        if v.Name == "Bit" then
            fireproximityprompt(v.ProximityPrompt)
        end
    end
wait(0.1)
end
	end    
})

